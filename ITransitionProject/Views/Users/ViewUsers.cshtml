@model IEnumerable<User>

<table class="table">
    <tr>
        <th>Имя</th>
        <th></th>
    </tr>
    @{
        foreach (User user in Model)
        {
            <tr>
                <td>@user.NotLoginName</td>
                <td><a asp-action="UserCollections" asp-route-userId="@user.Id" class="btn btn-primary">Просмотреть</a></td>
            </tr>
        }
    }
</table>